algoritmo "Descobrindo quantos anos"
var
   ano_nasc, ano_atual, resultado : inteiro
inicio
      escreva ("Informe o ano atual :")
      leia (ano_atual)
      escreva("Informe o ano em que nasceu :")
      leia (ano_nasc)
      resultado <- ano_atual - ano_nasc
      escreva("Você tem ",resultado, " Anos de idade!")

fimalgoritmo