algoritmo "Soma de dois numeros"
var
   n1, n2 , soma: inteiro
inicio
     escreva("Digite aqui Um numero Inteiro : ")
     leia (n1)
     escreva("Digite aqui um Segundo Numero Inteiro :")
     leia(n2)
     soma <- n1 + n2
     escreva ("A soma entre ",n1 ," e ",n2, " e igual a ",soma)
fimalgoritmo