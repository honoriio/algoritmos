algoritmo "Testes de condições"
var
   valor : real
inicio
      escreva("Informe aqui qual valor você tem guardado :")
      leia (valor)
      se (valor >= 10000) entao
           escreva ("Você tem o valor de R$",valor:5:2," Então Bora pra disney!!")
      senao
           escreval("Infelizmente não sera possivel ir a disney no momento :(")
      fimse
fimalgoritmo
